{  "dashboard": {    "title": "F1 Quantum-Aero Platform - Executive Overview",    "tags": ["f1", "quantum", "aerodynamics", "overview"],    "timezone": "browser",    "schemaVersion": 38,    "version": 1,    "refresh": "5s",    "panels": [      {        "id": 1,        "title": "Platform Status",        "type": "stat",        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},        "targets": [          {            "expr": "up{job=~\".*\"}",            "legendFormat": "{{job}}"          }        ],        "options": {          "graphMode": "none",          "colorMode": "background",          "textMode": "value_and_name"        },        "fieldConfig": {          "defaults": {            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "red"},                {"value": 1, "color": "green"}              ]            }          }        }      },      {        "id": 2,        "title": "Total API Requests/sec",        "type": "graph",        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},        "targets": [          {            "expr": "sum(rate(http_requests_total[5m]))",            "legendFormat": "Requests/sec"          }        ]      },      {        "id": 3,        "title": "AeroTransformer Inference Time (ms)",        "type": "graph",        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},        "targets": [          {            "expr": "histogram_quantile(0.95, rate(aerotransformer_inference_duration_seconds_bucket[5m])) * 1000",            "legendFormat": "p95"          },          {            "expr": "histogram_quantile(0.50, rate(aerotransformer_inference_duration_seconds_bucket[5m])) * 1000",            "legendFormat": "p50"          }        ],        "fieldConfig": {          "defaults": {            "unit": "ms",            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "green"},                {"value": 50, "color": "yellow"},                {"value": 100, "color": "red"}              ]            }          }        }      },      {        "id": 4,        "title": "GNN-RANS Speedup vs OpenFOAM",        "type": "stat",        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},        "targets": [          {            "expr": "gnn_rans_speedup_factor",            "legendFormat": "Speedup"          }        ],        "options": {          "graphMode": "area",          "colorMode": "value",          "textMode": "value_and_name"        },        "fieldConfig": {          "defaults": {            "unit": "x",            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "red"},                {"value": 1000, "color": "green"}              ]            }          }        }      },      {        "id": 5,        "title": "Quantum Job Queue",        "type": "graph",        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},        "targets": [          {            "expr": "quantum_job_queue_length{service=\"vqe-quantum\"}",            "legendFormat": "VQE Queue"          },          {            "expr": "quantum_job_queue_length{service=\"dwave-annealing\"}",            "legendFormat": "D-Wave Queue"          }        ]      },      {        "id": 6,        "title": "GPU Utilization",        "type": "graph",        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},        "targets": [          {            "expr": "nvidia_gpu_utilization_percent",            "legendFormat": "GPU {{gpu}}"          }        ],        "fieldConfig": {          "defaults": {            "unit": "percent",            "max": 100,            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "green"},                {"value": 80, "color": "yellow"},                {"value": 95, "color": "red"}              ]            }          }        }      },      {        "id": 7,        "title": "Generative Design - Candidates/Day",        "type": "stat",        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},        "targets": [          {            "expr": "sum(increase(diffusion_generations_total[24h]))",            "legendFormat": "Candidates"          }        ],        "options": {          "graphMode": "area",          "colorMode": "value"        },        "fieldConfig": {          "defaults": {            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "red"},                {"value": 1000, "color": "green"}              ]            }          }        }      },      {        "id": 8,        "title": "Digital Twin Sync Latency (ms)",        "type": "graph",        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},        "targets": [          {            "expr": "histogram_quantile(0.95, rate(digital_twin_sync_duration_seconds_bucket[5m])) * 1000",            "legendFormat": "p95"          }        ],        "fieldConfig": {          "defaults": {            "unit": "ms",            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "green"},                {"value": 100, "color": "yellow"},                {"value": 200, "color": "red"}              ]            }          }        }      },      {        "id": 9,        "title": "Telemetry Throughput (msg/s)",        "type": "graph",        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},        "targets": [          {            "expr": "rate(telemetry_messages_total[1m])",            "legendFormat": "Messages/sec"          }        ],        "fieldConfig": {          "defaults": {            "unit": "msg/s",            "thresholds": {              "mode": "absolute",              "steps": [                {"value": 0, "color": "red"},                {"value": 1000, "color": "green"}              ]            }          }        }      },      {        "id": 10,        "title": "Performance Targets Status",        "type": "table",        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},        "targets": [          {            "expr": "aerotransformer_inference_p95_ms < 50",            "legendFormat": "AeroTransformer <50ms",            "format": "table"          },          {            "expr": "gnn_rans_speedup_factor > 1000",            "legendFormat": "GNN-RANS >1000x",            "format": "table"          },          {            "expr": "vqe_circuit_depth < 100",            "legendFormat": "VQE <100 depth",            "format": "table"          },          {            "expr": "dwave_problem_size > 5000",            "legendFormat": "D-Wave >5000 vars",            "format": "table"          },          {            "expr": "diffusion_generation_time_seconds < 5",            "legendFormat": "Diffusion <5s",            "format": "table"          },          {            "expr": "digital_twin_sync_p95_ms < 100",            "legendFormat": "Digital Twin <100ms",            "format": "table"          },          {            "expr": "telemetry_optimization_time_seconds < 1",            "legendFormat": "Telemetry <1s",            "format": "table"          }        ],        "options": {          "showHeader": true        }      }    ]  }}