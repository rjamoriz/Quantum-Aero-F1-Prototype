# Master Orchestrator Agent DockerfileFROM python:3.11-slimWORKDIR /app# Install system dependenciesRUN apt-get update && apt-get install -y \    curl \    && rm -rf /var/lib/apt/lists/*# Copy requirementsCOPY ../requirements.txt .RUN pip install --no-cache-dir -r requirements.txt# Copy agent codeCOPY . .COPY ../utils /app/utilsCOPY ../config /app/config# Environment variablesENV PYTHONUNBUFFERED=1ENV PYTHONPATH=/app# Health checkHEALTHCHECK --interval=30s --timeout=10s --start-period=5s --retries=3 \  CMD curl -f http://localhost:6001/health || exit 1# Run agentCMD ["python", "-m", "agents.master_orchestrator.agent"]